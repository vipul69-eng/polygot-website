{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/public/locales/(.*)",
      "dest": "/public/locales/$1",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/docs/?$",
      "dest": "/website/docs/index.html"
    },
    {
      "src": "/?$",
      "dest": "/website/home/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
