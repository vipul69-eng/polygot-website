{
  "version": 2,
  "routes": [
    {
      "src": "/styles\\.css",
      "dest": "/styles.css"
    },
    {
      "src": "/script\\.js",
      "dest": "/script.js"
    },
    {
      "src": "/public/locales/(.*)",
      "dest": "/public/locales/$1"
    },
    {
      "src": "/(en|es|fr|de)/?$",
      "dest": "/home/index.html"
    },
    {
      "src": "/docs/?$",
      "dest": "/docs/index.html"
    },
    {
      "src": "/?$",
      "dest": "/home/index.html"
    }
  ]
}