{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/home/styles.css",
      "dest": "/home/styles.css",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/home/script.js",
      "dest": "/home/script.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/public/locales/(.*)",
      "dest": "/public/locales/$1",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(en|es|fr|de)/home",
      "dest": "/home/index.html"
    },
    {
      "src": "/(en|es|fr|de)/docs",
      "dest": "/docs/index.html"
    },
    {
      "src": "/(en|es|fr|de)",
      "dest": "/home/index.html"
    },
    {
      "src": "/home",
      "dest": "/home/index.html"
    },
    {
      "src": "/docs",
      "dest": "/docs/index.html"
    },
    {
      "src": "/",
      "dest": "/home/index.html"
    }
  ]
}