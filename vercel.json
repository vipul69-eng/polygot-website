{
  "version": 2,
  "routes": [
    { "src": "/locales/(.*)", "dest": "/public/locales/$1" },
    { "src": "/(en|es|fr|de)/home", "dest": "/home/index.html" },
    { "src": "/(en|es|fr|de)/docs", "dest": "/docs/index.html" },
    { "src": "/(en|es|fr|de)", "dest": "/home/index.html" },
    { "src": "/home", "dest": "/home/index.html" },
    { "src": "/docs", "dest": "/docs/index.html" },
    { "src": "/", "dest": "/home/index.html" }
  ]
}
